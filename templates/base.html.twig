        <link rel="stylesheet" href="/css/style.css"/>
        <link rel="stylesheet" href="/css/fontello.css"/>
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <div class="container-fluid">
            {% block header %}
            <div class="row">
